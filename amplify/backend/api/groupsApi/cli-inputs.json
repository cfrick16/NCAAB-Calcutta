{
  "version": 1,
  "paths": {
    "/groups": {
      "name": "/groups",
      "lambdaFunction": "groupsLambda",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/groups/*": {
      "name": "/groups/*",
      "lambdaFunction": "groupsLambda",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    }
  }
}